@model Lib.Services.Dtos.SubscribersDto

@{
    Layout = null;
}

<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title" id="modalCenterTitle">@Model.Firstname @Model.Lastname</h5>
        <button type="button"
                class="btn-close"
                data-bs-dismiss="modal"
                aria-label="Close"></button>
    </div>

    <div class="modal-body">
        
            <dl class="row mt-2">
                <dt class="col-sm-3">@Html.LabelFor(s=>s.Firstname)</dt>
            <dd class="col-sm-9">@Model.Firstname</dd>

                <dt class="col-sm-3">@Html.LabelFor(s=>s.Lastname)</dt>
            <dd class="col-sm-9">@Model.Lastname</dd>

                <dt class="col-sm-3">@Html.LabelFor(s=>s.Totalbook)</dt>
            <dd class="col-sm-9">@Model.Totalbook</dd>

                <dt class="col-sm-3">@Html.LabelFor(s=>s.CreatedAt)</dt>
                <dd class="col-sm-9">@Model.CreatedAt.Value.ToLocalTime().ToString("dd MMM yyyy")</dd>

                <dt class="col-sm-3">@Html.LabelFor(s=>s.UpdatedAt)</dt>
                <dd class="col-sm-9">@Model.UpdatedAt.Value.ToLocalTime().ToString("dd MMM yyyy")</dd>

            </dl>
        

    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal">
            Close
        </button>
    </div>

</div>

